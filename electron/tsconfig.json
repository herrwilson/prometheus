    "compilerOptions": {
      "target": "ESNext",
      "module": "commonjs",
      "lib": ["ESNext"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist"
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  